{
    "name": "skywest/installer",
    "description": "Installation package for all skywest specific projects.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Hilkiah Makemo",
            "email": "hilkiah.makemo@skywest.com"
        }
    ],
    "require": {

    },
    "autoload": {
        "psr-4": {
            "SKYW\\": "src/"
        }
    },
    "scripts": {
        "post-create-project-cmd": [
            "composer update",
            "@post-update-cmd"
        ],
        "post-install-cmd": [
            "@post-update-cmd"
        ],
        "post-update-cmd": [
            "alias skyw='bin/skyw' || doskey skyw='bin/skyw'",
            "skyw || true"
        ]
    },
    "minimum-stability": "dev"
}
